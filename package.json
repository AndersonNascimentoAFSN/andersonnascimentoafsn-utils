{
  "name": "andersonnascimentoafsn-utils",
  "description": "Package of utils function",
  "version": "0.0.2",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "homepage": "https://google.com",
  "engines": {
    "node": ">=20.0.0"
  },
  "repository": {
    "url": "https://github.com/AndersonNascimentoAFSN/andersonnascimentoafsn-utils"
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx,json}": [
      "eslint --fix --max-warnings=0"
    ],
    "src/**/*.{js,jsx,ts,tsx,json,css,scss,md}": [
      "prettier --write"
    ]
  },
  "scripts": {
    "build": "tsup src/index.ts --minify --dts --format cjs,esm --out-dir dist",
    "test:ci": "vitest --run",
    "test": "vitest",
    "test:coverage": "vitest run --coverage",
    "prepare": "husky install"
  },
  "keywords": [
    "utils"
  ],
  "author": "Anderson Nascimento",
  "license": "MIT",
  "devDependencies": {
    "@commitlint/cli": "^19.2.2",
    "@commitlint/config-conventional": "^19.2.2",
    "@vitest/coverage-istanbul": "^1.5.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "husky": "^8.0.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "jsdom": "^24.0.0",
    "lint-staged": "^15.2.2",
    "prettier": "^3.2.5",
    "tsup": "^8.0.2",
    "typescript": "^5.4.5",
    "vitest": "^1.5.0"
  },
  "dependencies": {
    "sharp": "^0.33.3"
  }
}
